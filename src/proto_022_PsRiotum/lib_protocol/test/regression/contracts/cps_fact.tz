{ storage nat ;
  parameter nat ;
  code { UNPAIR ;
         DIP { SELF ;
               ADDRESS ;
               SENDER ;
               { COMPARE ; EQ ; IF {} { DROP ; PUSH @storage nat 1 } } } ;
         DUP ;
         PUSH nat 1 ;
         { COMPARE ;
           GE ;
           IF { DROP ; NIL operation ; PAIR }
              { PUSH nat 1 ;
                SWAP ;
                SUB @parameter ;
                ISNAT ;
                IF_NONE
                  { NIL operation ; PAIR }
                  { DUP ;
                    DIP { PUSH nat 1 ; ADD ; MUL @storage } ;
                    SWAP ;
                    DIP { DIP { SELF ; PUSH mutez 0 } ;
                          TRANSFER_TOKENS ;
                          NIL operation ;
                          SWAP ;
                          CONS } ;
                    SWAP ;
                    PAIR } } } } }
