% :: nat : int : 'S   ->   option ( pair int nat ) : 'S
% :: nat : nat : 'S   ->   option (pair nat nat) : 'S

----- :: EDIV__0
EDIV / z1 : 0 : S  =>  None : S

z2 <> 0
