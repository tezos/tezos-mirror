            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "branch_delayed",
            "in": "query",
            "description": "Include branch delayed operations (true by default)",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "validation_pass",
            "in": "query",
            "description": "Include operations filtered by validation pass (all by default)",
            "required": false,
            "schema": {
              "type": "string"
            }
