
./octez-evm-node list events --json
applied_sequencer_upgrade:
  description: sequencer successfully upgraded to {sequencer} before blueprint {level}
  level: notice
  section: evm_node.dev
  json format:
    { /* applied_sequencer_upgrade version 0 */
      "applied_sequencer_upgrade.v0":
        { "sequencer": $Signature.Public_key,
          "level": $positive_bignum } }
    $Signature.Public_key:
      /* A Ed25519, Secp256k1, P256, BLS or Mldsa44 public key (Base58Check-encoded) */
      $unistring
    $positive_bignum:
      /* Positive big number
         Decimal representation of a positive big number */
      string
    $unistring:
      /* Universal string representation
         Either a plain UTF8 string, or a sequence of bytes for strings that contain invalid
         byte sequences. */
      string || { "invalid_utf8_string": [ integer ∈ [0, 255] ... ] }
  
applied_upgrade:
  description: kernel successfully upgraded to {root_hash} with blueprint {level}
  level: notice
  section: evm_node.dev
  json format:
