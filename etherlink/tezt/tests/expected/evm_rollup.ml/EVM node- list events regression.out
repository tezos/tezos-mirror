  level: error
  section: evm_node.dev
  json format:
    { /* blueprints_publisher_request_failed version 0 */
      "blueprints_publisher_request_failed.v0":
        { "view":
            { /* Publish */
              "request": "publish",
              "level": $positive_bignum,
              "payload":
                { /* Blueprint */
                  "chunks":
                    [ { "value": /^([a-zA-Z0-9][a-zA-Z0-9])*$/,
